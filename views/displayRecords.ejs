<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Social Media in Provo</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="/css/styles.css" rel="stylesheet" />
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- DataTables CSS -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.5/css/jquery.dataTables.css">
    <!-- DataTables JS -->
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.js"></script>
    <!-- TableFilter JS -->
    <script src="https://unpkg.com/tablefilter@latest/dist/tablefilter/tablefilter.js"></script>
    <!-- jQuery UI CSS -->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
</head>
<body>
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg bg-secondary text-uppercase fixed-top mt-0" id="mainNav">
        <div class="container">
            <a class="navbar-brand" href="/">Home</a>
            <button class="navbar-toggler text-uppercase font-weight-bold bg-primary text-white rounded" type="button" data-bs-toggle="collapse" data-bs-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                Menu
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ms-auto">
                    <form action="/addUser" method="GET">
                      <li class="nav-item mx-0 mx-lg-1"><button class="nav-link py-3 px-0 px-lg-3 rounded" type="submit">Create New Account</button></li>
                    </form>
                    <form action="/displayUser" method="GET">
                      <li class="nav-item mx-0 mx-lg-1"><button class="nav-link py-3 px-0 px-lg-3 rounded" type="submit">View Accounts</button></li>
                  </form>
                </ul>
            </div>
        </div>
    </nav>
    <br><br><br><br><br>
  <div class="tablecontainer">
   <table id="surveyTable" style="width: 100%;">
     <thead>
       <tr>
         <th>SurveyID</th>
         <th class="datepicker">Date</th>
         <th>Time</th>
         <th>Age</th>
         <th>Gender</th>
         <th>Relationship Status</th>
         <th>Occupation</th>
         <th>Uses SM?</th>
         <th>Avg. Hours on SM</th>
         <th>How Often SM Used w/o Purpose</th>
         <th>How Often Distracted by SM</th>
         <th>Restless if Not Used SM in a While?</th>
         <th>Ease of Distraction</th>
         <th>How Much Bothered by Worries</th>
         <th>Difficulty Concentrating</th>
         <th>How Often Compare Self to Others on SM</th>
         <th>General Feeling of Comparisons</th>
         <th>How Often Seeking Validation on SM</th>
         <th>How Often Depressed</th>
         <th>How Often Interest in Activities Fluctuating</th>
         <th>How Often Having Issues w/Sleep</th>
         <th>Location</th>
       </tr>
     </thead>
     <tbody>
      <% for (let iCount = 0; iCount < surveyentry.length; iCount++) { %>
         <tr>
           <td><%= surveyentry[iCount].SurveyID %></td>
           <td><%= surveyentry[iCount].Date.toLocaleDateString('en-US') %></td>
           <td><%= surveyentry[iCount].Time %></td>
           <td><%= surveyentry[iCount].Age %></td>
           <td><%= surveyentry[iCount].Gender %></td>
           <td><%= surveyentry[iCount].RelationshipStatus %></td>
           <td><%= surveyentry[iCount].OccupationStatus %></td>
           <td><%= surveyentry[iCount].UsesSocialMedia %></td>
           <td><%= surveyentry[iCount].AvgDailyHoursOnSocialMedia %></td>
           <td><%= surveyentry[iCount].RatingUsingSocialMediaWithoutPurpose %></td>
           <td><%= surveyentry[iCount].RatingGettingDistractedBySocialMediaWhenBusy %></td>
           <td><%= surveyentry[iCount].RatingRestlessWithoutSocialMedia %></td>
           <td><%= surveyentry[iCount].RatingEasilyDistracted %></td>
           <td><%= surveyentry[iCount].RatingBotheredByWorries %></td>
           <td><%= surveyentry[iCount].RatingDifficultyConcentrating %></td>
           <td><%= surveyentry[iCount].RatingComparesToOtherSuccessfulPeople %></td>
           <td><%= surveyentry[iCount].RatingFeelingsAboutComparisons %></td>
           <td><%= surveyentry[iCount].RatingSeekValidationFromSocialMedia %></td>
           <td><%= surveyentry[iCount].RatingDepression %></td>
           <td><%= surveyentry[iCount].RatingInterestFluctuation %></td>
           <td><%= surveyentry[iCount].RatingSleepIssues %></td>
           <td><%= surveyentry[iCount].Location %></td>
         </tr>
      <% } %>
     </tbody>
   </table>
  </div>
<script>
        $(document).ready(function () {
        $('#surveyTable').DataTable({
            
          });
        });

    var filtersConfig = {
        // instruct TableFilter location to import resources from
        base_path: 'https://unpkg.com/tablefilter@latest/dist/tablefilter/',
        col_1: '',
        col_2: '',
        col_3: '',
        col_4: 'select',
        col_5: 'select',
        col_6: 'select',
        col_7: 'select',
        col_8: 'select',
        col_9: 'select',
        col_11: 'select',
        col_10: 'select',
        col_12: 'select',
        col_13: 'select',
        col_14: 'select',
        col_15: 'select',
        col_16: 'select',
        col_17: 'select',
        col_18: 'select',
        col_19: 'select',
        col_20: 'select',
        col_21: 'select',
        alternate_rows: true,
        rows_counter: true,
        btn_reset: true,
        loader: true,
        mark_active_columns: true,
        highlight_keywords: true,
        no_results_message: true,
        matching_rows: 'all', // Add this line to search through all pages
        col_types: [
            'number', 'date_us', 'timeofdayh24',
            'number', 'string', 'string',
            'string', 'string', 'string',
            'number', 'number', 'number',
            'number', 'number', 'number',
            'number', 'number', 'number',
            'number', 'number', 'number',
            'string'
        ],
        extensions: [{
            name: 'sort',
            images_path: 'https://unpkg.com/tablefilter@latest/dist/tablefilter/style/themes/'
        }]
    };
    
    var tf = new TableFilter('surveyTable', filtersConfig);
    tf.init();
</script>
  <style>
    body {
        background-color: #f8f9fa; /* Set background color for the body */
        padding-left: 20px; /* Add left padding */
        padding-right: 20px; /* Add right padding */
    }

    th, td {
        border: 1px solid #dee2e6; /* Set the border color for table cells */
        padding: 8px;
        text-align: left;
    }

    tbody {
        font-family: Arial;
        font-size: 9pt;
    }
</style>
</body>
</html>